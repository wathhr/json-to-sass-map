{
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext"
    ],
    "baseUrl": "./src",
    "paths": {
      "#": ["./"],
      "#/*": ["./*"],
      "#structs": ["./structs/index.js"],
      "#structs/*": ["./structs/*.js"],
      "#utils": ["./utils/index.js"],
      "#utils/*": ["./utils/*.js"]
    }
  }
}
