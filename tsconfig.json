{
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outFile": "dist/index.js",
    "sourceMap": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext"
    ],
    "baseUrl": "./src",
    "paths": {
      "#structs": ["./structs"],
      "#structs/*": ["./structs/*"],
      "#utils": ["./utils"],
      "#utils/*": ["./utils/*"],
    }
  },
  "files": ["./src/index.ts"]
}
